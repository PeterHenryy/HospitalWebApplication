@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="zxx">

<head>
    <meta charset="UTF-8">
    <meta name="description" content="Aesthetic Template">
    <meta name="keywords" content="Aesthetic, unica, creative, html">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Online Hospital</title>

    <!-- Google Font -->
    <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@300;400;500;600;700;800;900&display=swap"
          rel="stylesheet">

    <!-- Css Styles -->
    <link rel="stylesheet" href="~/ProjectTheme/css/bootstrap.min.css" type="text/css">
    <link rel="stylesheet" href="~/ProjectTheme/css/font-awesome.min.css" type="text/css">
    <link rel="stylesheet" href="~/ProjectTheme/css/flaticon.css" type="text/css">
    <link rel="stylesheet" href="~/ProjectTheme/css/nice-select.css" type="text/css">
    <link rel="stylesheet" href="~/ProjectTheme/css/jquery-ui.min.css" type="text/css">
    <link rel="stylesheet" href="~/ProjectTheme/css/magnific-popup.css" type="text/css">
    <link rel="stylesheet" href="~/ProjectTheme/css/owl.carousel.min.css" type="text/css">
    <link rel="stylesheet" href="~/ProjectTheme/css/slicknav.min.css" type="text/css">
    <link rel="stylesheet" href="~/ProjectTheme/css/style.css" type="text/css">
    <style>
        .navbar-nav .nav-link:hover {
            color: rgb(92, 155, 217) !important;
        }
    </style>
</head>

<body>

    <!-- Offcanvas Menu End -->
    <!-- Header Section Begin -->
    <header class="header" style="margin-bottom: 10px; margin-top: 20px;">
        <div class="header-inner">
            <div class="container">
                <nav class="navbar navbar-expand-lg navbar-light" style="display: flex; align-items: center;">
                    <a class="navbar-brand" href="#">
                        <img style="height: 60px;" src="~/ProjectTheme/img/cliniclogo.png" alt="#">
                    </a>
                    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <div class="collapse navbar-collapse" id="navbarNav" style="justify-content: center;">
                        <ul class="navbar-nav ms-auto" style="gap: 15px; color: black; display: flex; align-items: center;">

                            <li class="nav-item">
                                <a class="nav-link" href="@Url.Action("Login", "AppUser")" style="color:black;">Home</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="@Url.Action("Login", "AppUser")" style="color:black;">About</a>
                            </li>

                            <li class="nav-item">
                                <a class="nav-link" href="@Url.Action("Login", "AppUser")" style="color:black;">Services</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="@Url.Action("Login", "AppUser")" style="color:black;">Pages</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="@Url.Action("Login", "AppUser")" style="color:black;">News</a>
                            </li>

                            <li class="nav-item">
                                <a class="nav-link" href="@Url.Action("Login", "AppUser")" style="color:black;">Login</a>
                            </li>
                            <li class="header__btn">
                                <a asp-action="Login" asp-controller="AppUser" class="primary-btn" style="color:black;">Book Appointment</a>
                            </li>


                        </ul>
                    </div>
                </nav>
            </div>
        </div>
    </header>
    <!-- Header Section End -->
    <!-- Hero Section Begin -->
    <section class="hero spad set-bg" data-setbg="https://back2healthpt.org/wp-content/uploads/2023/01/PhysicalTherapy.jpg">
        <div class="container">
            <div class="row">
                <div class="col-lg-6">
                    <div class="hero__text">
                        <span>Advanced Physical Treatment</span>
                        <h2>Take the world's best quality Treatment</h2>
                        <a asp-action="Login" asp-controller="AppUser" class="primary-btn normal-btn" style="background-color: rgb(92, 155, 217)">Join us!</a>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- Hero Section End -->
    <!-- Consultation Section Begin -->
    <!-- Consultation Section End -->
    <!-- Chooseus Section Begin -->
    <section class="chooseus spad">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <div class="section-title">
                        <span>Why choose us?</span>
                        <h2>Offer for you</h2>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-3 col-md-6 col-sm-6">
                    <div class="chooseus__item">
                        <img src="~/ProjectTheme/img/icons/ci-1.png" alt="">
                        <h5>Advanced equipment</h5>
                        <p>In the field of physical therapy, advanced equipment signifies a paradigm shift towards precision and efficacy in rehabilitation</p>
                    </div>
                </div>
                <div class="col-lg-3 col-md-6 col-sm-6">
                    <div class="chooseus__item">
                        <img src="~/ProjectTheme/img/icons/ci-2.png" alt="">
                        <h5>Qualified doctors</h5>
                        <p>Qualified doctors bring a wealth of expertise ensuring a high standard of medical professionalism and comprehensive healthcare delivery.</p>
                    </div>
                </div>
                <div class="col-lg-3 col-md-6 col-sm-6">
                    <div class="chooseus__item">
                        <img src="~/ProjectTheme/img/icons/ci-3.png" alt="">
                        <h5>Certified services</h5>
                        <p>Certified services in healthcare guarantee adherence to industry standards, and quality care across a range of medical disciplines</p>
                    </div>
                </div>
                <div class="col-lg-3 col-md-6 col-sm-6">
                    <div class="chooseus__item">
                        <img src="~/ProjectTheme/img/icons/ci-4.png" alt="">
                        <h5>Emergency care</h5>
                        <p>Emergency care is vital, delivering swift, specialized medical attention to critical situations, ensuring prompt interventions and potentially life-saving treatments.</p>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- Chooseus Section End -->
    <!-- Services Section Begin -->
    <section class="services spad set-bg" data-setbg="/ProjectTheme/img/services-bg.jpg">
        <div class="container">
            <div class="row">
                <div class="col-lg-8 col-md-8 col-sm-6">
                    <div class="section-title">
                        <span>Our services</span>
                        <h2>Offer for you</h2>
                    </div>
                </div>
                <div class="col-lg-4 col-md-4 col-sm-6">
                    <div class="services__btn">
                        <a asp-action="Login" asp-controller="AppUser" class="primary-btn" style="background-color: rgb(92, 155, 217)">Join us</a>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-6 col-md-6">
                    <div class="services__item">
                        <div class="services__item__icon">
                            <img width="75px" src="~/logo/human.png">
                        </div>
                        <div class="services__item__text">
                            <h5>Pulmonary</h5>
                            <p>
                                The branch of medicine that deals with the causes, diagnosis, prevention and treatment of diseases affecting the lungs.
                            </p>
                        </div>
                    </div>
                </div>
                <div class="col-lg-6 col-md-6">
                    <div class="services__item">
                        <div class="services__item__icon">
                            <img width="75px" src="~/logo/neurology.png">
                        </div>
                        <div class="services__item__text">
                            <h5>Neurology</h5>
                            <p>
                                The branch of medicine that deals with the diagnosis and treatment of disorders of the nervous system.
                            </p>
                        </div>
                    </div>
                </div>
                <div class="col-lg-6 col-md-6">
                    <div class="services__item">
                        <div class="services__item__icon">
                            <img width="75px" src="~/logo/heart-disease.png">
                        </div>
                        <div class="services__item__text">
                            <h5>Cardiovascular </h5>
                            <p>
                                The branch of medicine that pertaining to the health of the circulatory and respiratory systems.
                            </p>
                        </div>
                    </div>
                </div>
                <div class="col-lg-6 col-md-6">
                    <div class="services__item">
                        <div class="services__item__icon">
                            <img width="75px" src="~/logo/orthopedics.png">
                        </div>
                        <div class="services__item__text">
                            <h5>Orthopedic </h5>
                            <p>
                                The branch of medicine therapy that focuses on the musculoskeletal system, including joints, muscles, bones, ligaments, and tendons.
                            </p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- Gallery Begin -->
    <div class="gallery">
        <div class="gallery__container">
            <div class="grid-sizer"></div>
            <div class="gc__item set-bg" data-setbg="https://cdn.careeronestop.org/OccVids/OccupationVideos/29-1123.00.jpg">
            </div>
            <div class="gc__item set-bg" data-setbg="https://newhealthtimes.s3.amazonaws.com/uploads/article_images/What%20do%20exercise%20phy1593397285.jpg">
            </div>
            <div class="gc__item set-bg" data-setbg="https://explorehealthcareers.org/wp-content/uploads/2016/11/physical_therapist.jpg">
            </div>
            <div class="gc__item gc__item__large set-bg" data-setbg="https://i.pinimg.com/originals/b2/33/33/b23333f4089e362d308ebdc852623f10.jpg">
            </div>
            <div class="gc__item set-bg" data-setbg="https://www.orthobethesda.com/wp-content/uploads/2020/04/Knee-Therapy.jpg">
            </div>
            <div class="gc__item set-bg" data-setbg="https://www.athletico.com/wp-content/uploads/2017/07/Physical-Therapy-for-Low-Back-Pain-Effective-Treatment-and-Lower-Costs-3.jpg">
            </div>
            <div class="gc__item set-bg" data-setbg="https://1.bp.blogspot.com/-FMhuV5gFybk/XdPp2t1ERtI/AAAAAAAAPXY/jKOM7hR2RNEgyHNWyMuTz-WU58PrmG3UgCLcBGAsYHQ/s400/Know%2Bthe%2Brole%2Bof%2Bphysiotherapist%2Band%2Bthe%2Bconditions%2Bit%2Bhandled.jpg">
            </div>
        </div>
    </div>
    <!-- Gallery End -->
    <!-- Latest News Begin -->
    <!-- Latest News End -->
    <!-- Footer Section Begin -->
    <footer class="footer">
        <div class="footer__top">
            <div class="container">
                <div class="row">

                    <div class="col-lg-12 col-md-12">
                        <div class="footer__newslatter">
                            <a style="color:white;margin-right:30px; ">Welcome to Ineza Physiotherapy  </a><a style="background-color: rgb(92, 155, 217);" class="site-btn" asp-action="Login" asp-controller="AppUser">Join Us</a>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </footer>
    <!-- Footer Section End -->
    <!-- Js Plugins -->
    <script src="~/ProjectTheme/js/jquery-3.3.1.min.js"></script>
    <script src="~/ProjectTheme/js/bootstrap.min.js"></script>
    <script src="~/ProjectTheme/js/jquery.magnific-popup.min.js"></script>
    <script src="~/ProjectTheme/js/masonry.pkgd.min.js"></script>
    <script src="~/ProjectTheme/js/jquery-ui.min.js"></script>
    <script src="~/ProjectTheme/js/jquery.nice-select.min.js"></script>
    <script src="~/ProjectTheme/js/jquery.slicknav.js"></script>
    <script src="~/ProjectTheme/js/owl.carousel.min.js"></script>
    <script src="~/ProjectTheme/js/main.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>
</body>

</html>